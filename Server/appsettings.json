{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Authentication": "Debug",
      "Microsoft.AspNetCore.Authorization": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "arrangley",
          "https://auth.arrangely.net/resources"
        ],
        "AuthAuthority": "https://auth.arrangely.net",
        "MetadataAddress": "https://auth.arrangely.net/.well-known/openid-configuration"
      }
    }
  }
}
